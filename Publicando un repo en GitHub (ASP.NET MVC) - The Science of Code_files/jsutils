function getDefaults(n){var t=n||{};return t.load=t.load||!1,t.push=t.push||!1,t.top=t.top||0,t.div=t.div||"",t.func=t.func||defaultLoad,t.precall=t.precall||undefined,t.postcall=t.postcall||undefined,t.method=t.method||"POST",t.contentType=t.contentType||"text/html; charset=utf-8",t.getCall=t.getCall||function(n){return $(n).data("url")},t.updateMetadata=t.updateMetadata||updateMetadata,t.metadataDesc=t.metadataDesc||"#desc",t.metadataKeywords=t.metadataKeywords||"#keywords",t.metadataTitle=t.metadataTitle||"#title",t.metadataImageUrl=t.metadataImageUrl||"#mainImageUrl",t}function setOnClickDataUrl(n){var t=getDefaults(n);t.push&&(window.onpopstate=function(){firstHistory||(document.location=document.location)});$("[data-url]").unbind("click");$("[data-url]").on("click",function(n){n.preventDefault();t.push&&(firstHistory=!1,window.history.pushState({js:!0},"",$(this).data("url")));t.precall&&t.precall();$("html, body").animate({scrollTop:t.top});t.func($(this),t);t.postcall&&t.postcall()})}function defaultLoad(n,t){for(i=0;i<reqs.length;i++)reqs[i].abort();reqs.length=0;t.load&&jQuery(n).find("img").addClass("visible");var r=$.ajax({type:t.method,contentType:t.contentType,url:t.getCall(n),success:function(i){if($(t.div).html(i),setOnClickDataUrl(t),t.push){var r=$(t.metadataDesc).html(),u=$(t.metadataKeywords).html(),f=$(t.metadataTitle).html(),e=$(t.metadataImageUrl).html();t.updateMetadata(f,r,u,e)}t.load&&jQuery(n).find("img").removeClass("visible");try{FB.XFBML.parse();twttr.widgets.load()}catch(o){}}});reqs.push(r)}function updateMetadata(n,t,i,r){$("title").html(n);$("meta[name=description]").remove();$("head").append('<meta name="description" content="'+t+'" />');$("meta[name=keywords]").remove();$("head").append('<meta name="keywords" content="'+i+'" />');$('meta[property="og:description"]').remove();$("head").append('<meta property="og:description" content="'+t+'" />');$('meta[property="og:title"]').remove();$("head").append('<meta property="og:title" content="'+n+'" />');$('meta[property="og:image"]').remove();$("head").append('<meta property="og:url" content="'+r+'" />')}var fixed_menu,fixed_status,called,firstHistory,reqs;$(function(){$(window).scroll(function(){var n;$(window).scrollTop()>100?(n=$(".fixeable-navbar"),n.is(":visible")||n.fadeIn()):(n=$(".fixeable-navbar"),n.is(":visible")&&n.fadeOut())})});$(function(){$("a[data-hideable]").click(function(){var n=$(this).data("hideable"),t=$(this).data("expected-result");$.ajax({type:"POST",contentType:"application/json; charset=utf-8",url:$(this).data("callable-url"),success:function(i){i==t&&$(n).fadeOut("fast")}})})});fixed_menu=-1;fixed_status=!1;$(function(){$("#fixeable-menu").offset()!=undefined&&(fixed_menu=$("#fixeable-menu").offset().top-45,$("#fixed-menu").html($("#fixeable-menu").html()),$(window).scroll(function(){if($(window).scrollTop()>fixed_menu){if(fixed_status)return;fixed_status=!0;$("#fixed-menu").css("opacity","1");$("a[data-toggle]").removeClass("btn-primary");$("a[data-toggle]").addClass("btn-default")}else{if(!fixed_status)return;fixed_status=!1;$("#fixed-menu").css("opacity","0");$("a[data-toggle]").removeClass("btn-default");$("a[data-toggle]").addClass("btn-primary")}}))});called=!1;$(function(){$("a[data-toggle]").click(function(){called||(called=!0,$('[data-showable="true"]').addClass("min-h-content"));var n=$("a[data-toggle="+$(this).data("toggle")+"]"),t=!0;n.hasClass("active")&&(n.removeClass("active"),t=!1);$("a[data-toggle]").removeClass("active");$("html, body").animate({scrollTop:fixed_menu-40});$.when($('[data-hide-me="true"]').fadeOut("fast")).then(function(){if(t){var i=n.attr("data-toggle");n.addClass("active");$(i).fadeIn("fast")}})})});firstHistory=!0;reqs=[];$(document).ready(function(){$('a.smooth[href^="#"]').on("click",function(n){n.preventDefault();var t=this.hash,i=$(t);$("html, body").stop().animate({scrollTop:i.offset().top-60},900,"swing",function(){window.location.hash=t})})})